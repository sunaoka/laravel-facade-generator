{
    "name": "sunaoka/laravel-facade-generator",
    "description": "Provide command line generation of facade layer files.",
    "keywords": ["laravel", "facade", "artisan", "generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\LaravelFacadeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\LaravelFacadeGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sunaoka\\LaravelFacadeGenerator\\FacadeGeneratorServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
